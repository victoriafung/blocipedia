<%= form_for @collaborator do |f| %>
  Add Collaborators
  <div class="form-group">
    <%= f.label :user_id %>
    <%= f.collection_select :user_id, User.all, :id, :email  %>
    <%= f.hidden_field :wiki_id, :value => @wiki.id %>
  </div>
  <div class="form-group">
    <%= f.submit "Add Collaborator", class: 'btn btn-success' %>
  </div>
<% end %>
