<h1><%= @wiki.markdown_title %></h1>
 <div class="row">
   <div class="col-md-8">
     <p><%= @wiki.markdown_body %></p>
   </div>
   <div class="col-md-4">
     <% if current_user.admin? || current_user.premium? || current_user = @wiki.user %>
       <%= link_to "Edit", edit_wiki_path(@wiki), class: 'btn btn-success' %>
       <%= link_to "Delete", @wiki, method: :delete, class: "btn btn-danger", data: {confirm: "Are you sure you want to delete this Wiki?"} %>
     <% end %>
   </div>
 </div>
